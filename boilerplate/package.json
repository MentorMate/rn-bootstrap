{
  "name": "boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "check-circular-deps": "yarn dpdm --circular --tsconfig tsconfig.json --tree=false --warning=false --exit-code circular:1 ./*",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "pod-clean-install": "(cd ios && rm -rf Pods && pod install)",
    "pod-install": "(cd ./ios && pod install)",
    "prepare": "husky install",
    "prettier:check-format": "prettier --list-different",
    "prettier:format": "prettier --write",
    "setup": "yarn install && yarn pod-install",
    "start": "react-native start",
    "test": "jest --maxWorkers=2",
    "test:clear:cache": "jest --clearCache",
    "test:coverage": "jest --maxWorkers=2 --collectCoverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "validate:commit-message": "commitlint --edit $1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "react-native"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/netinfo": "^7.1.7",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-gesture-handler": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/react-native": "^9.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.4",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "dpdm": "^3.8.0",
    "eslint": "^8.6.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "jest-expo": "^44.0.1",
    "lint-staged": "^12.1.7",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.5.1",
    "react-test-renderer": "17.0.2",
    "sort-package-json": "^1.53.1",
    "typescript": "^4.4.4"
  }
}

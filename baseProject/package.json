{
  "name": "baseproject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "check-circular-deps": "yarn dpdm --circular --tsconfig tsconfig.json --tree=false --warning=false --exit-code circular:1 ./*",
    "ios": "react-native run-ios",
    "ios-dev": "react-native run-ios --scheme \"baseProjectDev\"",
    "ios-stage": "react-native run-ios --scheme \"baseProjectStage\"",
    "ios-prod": "react-native run-ios --scheme \"baseProject\"",
    "android:dev": "react-native run-android --variant=devDebug",
    "android:dev-release": "react-native run-android --variant=devRelease",
    "android:stage": "react-native run-android --variant=stageDebug",
    "android:stage-release": "react-native run-android --variant=stageRelease",
    "android:prod": "react-native run-android --variant=prodDebug",
    "android:prod-release": "react-native run-android --variant=prodRelease",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "pod-clean-install": "(cd ios && rm -rf Pods && pod install)",
    "pod-install": "(cd ./ios && pod install)",
    "prepare": "husky install",
    "prettier:check-format": "prettier --list-different",
    "prettier:format": "prettier --write",
    "setup": "yarn install && yarn pod-install",
    "start": "react-native start",
    "start:reset-cache": "react-native start --reset-cache",
    "test": "jest --maxWorkers=2",
    "test:clear:cache": "jest --clearCache",
    "test:coverage": "jest --maxWorkers=2 --collectCoverage",
    "test:open-coverage-report": "open ./coverage/lcov-report/index.html",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:ci": "yarn test:coverage --ci --silent --maxWorkers=2",
    "typecheck": "tsc --noEmit",
    "validate:commit-message": "commitlint --edit $1",
    "npm:version:patch": "npm version patch --no-git-tag-version",
    "npm:version:minor": "npm version minor --no-git-tag-version",
    "npm:version:major": "npm version major --no-git-tag-version",
    "fastlane:update-versions": "bundle exec fastlane update_versions",
    "version:build": "yarn fastlane:update-versions",
    "version:patch": "yarn npm:version:patch --no-git-tag-version && yarn fastlane:update-versions",
    "version:minor": "yarn npm:version:minor --no-git-tag-version && yarn fastlane:update-versions",
    "version:major": "yarn npm:version:major && yarn fastlane:update-versions",
    "prettify:write": "prettier --write \"**/*.{json,js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.7",
    "@react-native-community/netinfo": "^9.3.0",
    "react": "18.2.0",
    "react-native": "0.69.3",
    "react-native-config": "^1.4.6",
    "react-native-gesture-handler": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@testing-library/react-native": "^10.1.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/jest-native": "^4.0.5",
    "@types/jest": "^26.0.24",
    "@types/react-native": "^0.69.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "dpdm": "^3.9.0",
    "husky": "^8.0.1",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.3",
    "metro-react-native-babel-preset": "^0.71.3",
    "prettier": "^2.7.1",
    "react-test-renderer": "18.2.0",
    "sort-package-json": "^1.57.0",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}